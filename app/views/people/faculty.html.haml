= render 'shared/welcome'
%table#people.bordered.zebra
  %tr
    %th N#
    - if Faculty.column_names.include? :initials
      %th Initials
    %th Kvalifikaciya
    %th Nazva
    %th Specialnist
    %th Person ID
  - @faculty.each_with_index do |person, i|
    %tr
      %td= i+1
      - if Faculty.column_names.include? :initials
        %th Initials
      %td= person.kvalifikaciya
      %td= person.faculty_name
      %td= person.specialnist
      %td= person.person_id



